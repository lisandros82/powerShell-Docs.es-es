{
  "build": {
    "markdownEngineName": "markdig",
    "content": [
      {
        "files": [
          "**/**.md",
          "**/**.yml"
        ],
        "exclude": [
          "**/obj/**"
        ]
      }
    ],
    "globalMetadata": {
      "ROBOTS": "INDEX, FOLLOW",
      "ms.prod": "powershell",
      "ms.technology": "powershell",
      "ms.topic": "conceptual",
      "manager": "carmonm",
      "ms.devlang": "powershell",
      "ms.tgt_pltfr": "windows, macos, linux",
      "author": "juanpablojofre",
      "ms.author": "jpjofre",
      "bilingual_type": "hover over",
      "disable_contribution_on_translation_quality": [
        "MT",
        "MTE75",
        "MTE95",
        "Blank"
      ]
    },
    "resource": [
      {
        "files": [
          "**/images/**"
        ],
        "exclude": [
          "**/obj/**"
        ]
      }
    ],
    "template": "op.html",
    "dest": "scripting"
  }
}